@import "./reset.css"

.flex-cont{
  display : -webkit-box;
  display : -webkit-flex;
  display : flex;
  flex-wrap:nowrap;
  flex-direction:row;
}

.flex-item-1{
  -webkit-box-flex:1;
  -webkit-flex:1;
  flex:1;
}

.flex-item-2{
  -webkit-box-flex:2;
  -webkit-flex:2;
  flex:2;
}

.clearfix:after{display:block;clear:both;content:"";visibility:hidden;height:0}
.clearfix{zoom:1}
  
    